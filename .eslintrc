{
	"ecmaVersion": 5,
	"env": {
		"browser": true
	},
	"globals": {
		"angular": true,
		"jasmine": true,
		"module": true,
		"inject": true,
		"beforeEach": true,
		"afterEach": true,
		"beforeAll": true,
		"afterAll": true,
		"spyOn": true,
		"describe": true,
		"expect": true,
		"require": true,
		"it": true,
		"_": true
	},
	"parserOptions": {
		"ecmaFeatures": {
			"impliedStrict": true
		},
		"ecmaVersion": 5
	},
	"rules": {
		"block-scoped-var": 2,
		"camelcase": 2,
		"comma-style": [2, "last"],
		"curly": [2, "all"],
		"dot-notation": 2,
		"eqeqeq": 2,
		"guard-for-in": 2,
		"no-bitwise": 2,
		"no-caller": 2,
		"no-cond-assign": [2, "except-parens"],
		"no-debugger": 2,
		"no-labels": 2,
		"no-eval": 2,
		"no-extend-native": 2,
		"no-irregular-whitespace": 2,
		"no-iterator": 2,
		"no-loop-func": 2,
		"no-multi-str": 2,
		"no-multiple-empty-lines": [2, {
			"max": 1
		}],
		"no-new-symbol": 2,
		"no-plusplus": 0,
		"no-proto": 2,
		"no-script-url": 2,
		"no-sequences": 2,
		"no-shadow": 2,
		"no-undef": 2,
		"no-unused-vars": 2,
		"no-with": 2,
		"quotes": [2, "single"],
		"brace-style" : [2, "1tbs"],
		"semi": [0, "never"],
		"strict": 2,
		"valid-typeof": 2,
		"wrap-iife": [2, "outside"],
		"indent": [2, "tab"],
		"eol-last": [2, "never"],
		"no-extra-semi": 2,
		"no-spaced-func": 2,
		"no-trailing-spaces": 2,
		"space-before-blocks": [2, "always"],
		"space-infix-ops": 2,
		"space-before-function-paren": [2, "never"],
		"space-in-parens": [2, "always"],
		"keyword-spacing": [2, {
			"before": true
		}]
	},
	"plugins": ["angular"]
}